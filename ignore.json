{
  "reasons": [
    "DDoc doesn't support indented code blocks",
    "`---` indicates a code block in DDoc, not a heading or thematic break",
    "DDoc code blocks that start with `---` must also end with `---`"
  ],
  "examples": {
    "1": { "reason": 1 },
    "2": { "reason": 1 },
    "3": { "reason": 1 },
    "5": { "reason": 1 },
    "6": { "reason": 1 },
    "7": { "reason": 1 },
    "8": { "reason": 1 },
    "18": { "reason": 1 },
    "39": { "reason": 1 },
    "69": { "reason": 1 },
    "76": { "reason": 1 },
    "25": {
      "reason": 2,
      "exclude": true
    },
    "97": {
      "reason": 3,
      "exclude": true
    }
  }
}